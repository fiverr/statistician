    "fingerprinting": "^1.0.1",
    "index-require": "^0.1.0",
    "marked": "^0.5.1",
    "node-fetch": "^2.2.0",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "chai-string": "^1.5.0",
    "clear-module": "^3.0.0",
    "eslint": "^5.7.0",
    "mocha": "^5.2.0"
  }
}
