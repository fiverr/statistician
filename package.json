{
  "name": "statistician",
  "version": "0.5.2",
  "description": "Create and compare files stats, and webpack bundle stats",
  "keywords": [
    "webpack-stats",
    "file-size",
    "pull-requests",
    "package",
    "bundle",
    "analyze",
    "compare-files",
    "sizes"
  ],
  "author": "Fiverr SRE",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fiverr/statistician.git"
  },
  "homepage": "https://github.com/fiverr/statistician/",
  "main": "index.js",
  "bin": "cli.js",
  "engines": {
    "node": ">=8"
  },
  "preferGlobal": true,
  "scripts": {
    "start": "node index",
    "test": "find . | grep spec.js | grep -v node_modules | xargs mocha --require .mochaenv.js",
    "lint": "eslint -c .eslintrc '*.js' '**/*.js' --quiet"
  },
  "dependencies": {
    "await-reduce": "^1.2.1",
    "byte-size": "^4.0.4",
    "chai": "^4.2.0",
